   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="exporta_consulta_pdf" meta_type="CMF Action">
   <property name="title">Exportar PDF</property>
   <property name="description">Exporta a consulta pública em formato PDF</property>
